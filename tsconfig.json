{
  "compilerOptions": {

    "target": "es5",
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "./dist",


   "strict": true,
   "strictPropertyInitialization": false,
   "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
